{
  "name": "paradigm-dental-platform",
  "version": "1.0.0",
  "description": "SaaS platform for dental prosthesis ordering between dentists and suppliers",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "build": "cd client && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "node server/index.js",
    "init-db": "node server/scripts/init-db.js",
    "reset-db": "node server/scripts/init-db.js"
  },
  "keywords": ["dental", "saas", "prosthesis", "ordering", "platform"],
  "author": "Paradigm Laboratory",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "dotenv": "^16.3.1",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "cloudinary": "^1.41.0",
    "socket.io": "^4.7.4",
    "pg": "^8.11.3",
    "sequelize": "^6.35.2",
    "nodemailer": "^6.9.7",
    "axios": "^1.6.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "compression": "^1.7.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}